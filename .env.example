# Frontend Environment Variables Template
# Copy this file to .env.production for your local production build
# DO NOT commit actual .env files to GitHub!

# Backend API URL
# Development: leave empty to use Vite proxy (connects to localhost:5000)
# Production: set this in your deployment platform (Vercel/Netlify) environment variables
VITE_API_BASE_URL=https://moodyserver.onrender.com

# App Environment
VITE_NODE_ENV=production

# DEPLOYMENT INSTRUCTIONS:
# 1. Never commit .env files to GitHub
# 2. In your deployment platform (Vercel/Netlify), set:
#    VITE_API_BASE_URL=https://moodyserver.onrender.com
# 3. For local development, leave VITE_API_BASE_URL empty
